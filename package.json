{
    "name": "asteroids-clone",
    "type": "module",
    "description": "An Asteroids clone with mouse aiming built with Phaser 3",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write src/",
        "format:check": "prettier --check src/"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "prettier": "^3.7.4",
        "terser": "^5.39.0",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.52.0",
        "vite": "^6.3.1"
    },
    "dependencies": {
        "phaser": "^3.90.0"
    }
}